  "main": "lib/tingodb-driver.js",
  "author": "Mark 'kaosat-dev' Moissette",
  "license": "MIT",
  "dependencies": {
    "@cycle/core": "^5.0.1",
    "@cycle/dom": "^7.1.1",
    "rx": "^4.0.6",
    "tingodb": "^0.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "mocha": "^2.3.4",
    "watchify": "^3.6.1"
  },
  "scripts": {
    "browserify": "browserify src/tingodb-driver  -t babelify --o dist/cycle-tingodb.js",
    "build": "npm run browserify",
    "compile-lib": "babel -d lib/ src/",
    "watch": "watchify src/tingodb-driver  -t babelify --o dist/cycle-tingodb.js -v -d",
    "mocha": "mocha test --compilers js:babel-core/register",
    "test": "npm run mocha",
    "release": "npm run release-patch",
    "release-patch": "git checkout master ; npm version patch && git push origin master --tags ",
    "release-minor": "git checkout master ; npm version patch && git push origin master --tags ",
    "release-major": "git checkout master ; npm version patch && git push origin master --tags "
  },
  "browserify-shim": {
    "rx": "global:Rx"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
